@model UdemyProject.ViewModels.RandomMovieViewModel

@{
    ViewBag.Title = "RandomMovie";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Movie</h2>
@if (Model.Customer.Count == 0)
{
    <text>No customer rent this movie</text>
}
else
{
<ul>
    @foreach (var cus in Model.Customer)
    {
        <li>@cus.Name</li>
    }
</ul>
}

